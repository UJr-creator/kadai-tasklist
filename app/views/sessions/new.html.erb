<h1>Log in</h1>
  <%= form_with(url: login_path, scope: :session, local: :true) do |f| %>
  <%= render "layouts/error_messages", model: f.object %>
  
  <%= f.label :email, "Email" %>
  <%= f.email_field :email %>
  
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>
  
  <%= f.submit "Log in" %>

<p>New User? <%= link_to "Sign up now!", signup_path %></p>

<% end %>